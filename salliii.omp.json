{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "palette": {
    "basicColor": "#ffffff",
    "pathWritable": "#7fffdf",
    "pathUnwritable": "#ff5050",
    "exitFailed": "#ff0000",
    "exitPassed": "#00ff00",
    "cmdPrompt": "#ffb545",
    "dimmed": "#6c6c6c"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:basicColor",
          "template": "{{ .UserName }}  "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:basicColor",
          "template": "{{ if .Writable }}<p:pathWritable>{{ else }}<p:pathUnwritable>{{ end }}\uf114 {{ if .WSL }}[wsl]{{ end }}{{ .Path }}</>",
          "properties": {
            "style": "full",
            "mapped_locations_enabled": false,
            "home_icon": ""
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:basicColor",
          "template": "<p:dimmed>Â·</><d>{{ .HEAD }}</d>",
          "properties": {
            "fetch_status": true,
            "branch_icon": ""
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:exitPassed",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:exitFailed{{ end }}"
          ],
          "template": " {{ if gt .Code 0 }}\u2a2f{{else}}\u2713{{ end }}",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:dimmed",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:dimmed",
          "template": " Â· {{.CurrentDate | date .Format}}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:cmdPrompt",
          "template": "{{ if .Root }}ðŸ’€{{ end }}<p:dimmed>Â·</>\u276f"
        }
      ]
    }
  ]
}