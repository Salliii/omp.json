{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "palette": {
    "osIcon": "#47edff",
    "sessionUser": "#47edff",
    "exitFailed": "#ff0000",
    "exitPassed": "#00ff00",
    "cmdPrompt": "#ff8945",
    "dimmed": "#6c6c6c"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:osIcon",
          "template": "{{ .Icon }}  <p:dimmed>\u276f</> ",
          "properties": {
            "kali": "\uf327"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:sessionUser",
          "template": "{{ .UserName }}  <p:dimmed>\u276f</> "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:cmdPrompt",
          "template": "{{ if .Root }}ðŸ’€{{ end }}#"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:exitPassed",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:exitFailed{{ end }}"
          ],
          "template": " {{ if gt .Code 0 }}\u2a2f{{else}}\u2713{{ end }}",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:dimmed",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "always_enabled": true,
            "style": "austin"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:dimmed",
          "template": " Â· {{.CurrentDate | date .Format}}",
          "properties": {
            "time_format": "03:04:05"
          }
        }
      ]
    }
  ]
}